  "dependencies": {
    "eslint-config-cheminfo": "^11.0.4",
    "eslint-plugin-deprecation": "^3.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "typescript": ">=5.5.4"
  },
  "devDependencies": {
    "cheminfo-types": "^1.7.3",
